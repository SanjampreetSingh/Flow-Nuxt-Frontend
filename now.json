{
    "version": 2,
    "builds": [{
        "src": "nuxt.config.js",
        "use": "@nuxtjs/now-builder",
        "config": {}
    }],
    "alias": [
        "theflowai.com",
        "www.theflowai.com"
    ],
    "routes": [{
            "src": "/_nuxt/.+",
            "headers": {
                "cache-control": "s-maxage=31536000"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/_nuxt",
            "dest": "/404.html",
            "status": 404
        },
        {
            "src": "/_nuxt/img/",
            "dest": "/404.html",
            "status": 404
        }
    ]
}